doctype html
html(lang="ru")
    head
        meta(charset="utf-8")
        meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
        title Road2eSports
        link(rel="stylesheet" href="/style/main.css")
        link(rel="shortcut icon" type="image/png" href="/img/icon_32.png" sizes="32x32")
        link(rel="shortcut icon" type="image/png" href="/img/icon_16.png" sizes="16x16")
    body
        .container
            .row
                div.input
                    h2 Дата начала
                    form(action="#" method="post" id="input-form")
                        div.form-line
                            table
                                tbody
                                    tr.input-block.input-border
                                        td
                                            input(type="text" class="user-input" id="start-day" name="start-day" maxLength="2" autocomplete="off" data-min="1" data-max="31")
                                        td.input-delimiter /
                                        td
                                            input(type="text" class="user-input" id="start-month" name="start-month" maxLength="2" autocomplete="off" data-min="1" data-max="12")  
                                        td.input-delimiter /
                                        td
                                            input(type="text" class="user-input year" id="start-year" name="start-year" maxLength="4" autocomplete="off" data-min="2016" data-max="2050") 
                                        td
                                            a#show-dp(href="javascript:void(0)" data-event="show:dpicker")
                                                img(alt="календарь" src="data:image/png;base64,")
                        
                        
                        
                        
                        div.form-line
                            table
                                tbody
                                    tr.input-block.input-border
                                        td
                                            input(type="text" class="user-input" id="start-hours" name="start-hours" maxLength="2" autocomplete="off" data-min="0" data-max="23")
                                        td.input-delimiter :
                                        td
                                            input(type="text" class="user-input" id="start-min" name="start-min" maxLength="2" autocomplete="off" data-min="0" data-max="59")  

                        div.form-line
                            h2 Контрольное время
                        div.form-line.last
                            table
                                tbody
                                    tr.input-block.input-border
                                        td
                                            input(type="text" class="user-input wide" id="target-hours" name="target-hours" maxLength="3" autocomplete="off" data-min="0" data-max="220" data-auto="100")
                                        td.input-delimiter :
                                        td
                                            input(type="text" class="user-input" id="target-min" name="target-min" maxLength="2" autocomplete="off" data-min="0" data-max="59")
                        div.form-line.center
                            input(class="submit-btn btn" type="submit" value="расчет")
            include ./dpicker.pug
            include ./result.pug
        block scripts
            script(src="/js/core.js")
            script(src="/js/events.js")
            script(src="/js/input.js")
            script(src="/js/result.js")
            script(src="/js/datepicker.js")
            script(src="/js/app.js")